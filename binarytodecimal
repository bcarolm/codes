#transformar de binário para decimal

from pythonds.basic.queue import Queue

def Bin_to_Dec(num): #lógica FIFO
    fila = Queue()
    soma =  0
    for elem in num:
        fila.enqueue(elem)
    while not fila.isEmpty():
        index = fila.size() - 1
        removal = fila.dequeue()
        soma+= int(removal)*2**(index)
    return soma

        
